@model BASE_COBRANZAS_V2.Models.Beans.Status_demanda_principal

@{
    ViewData["Title"] = "Demanda Principal";
    Layout = "~/Views/Shared/_layoutMstr.cshtml";
}

<h1 class="font-weight-bold text-center">Registrar</h1>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group" style="display:none">
                <label asp-for="ID_STATUS_DEMANDA_PRINCIPAL" class="control-label"></label>
                <input asp-for="ID_STATUS_DEMANDA_PRINCIPAL" class="form-control" />
                <span asp-validation-for="ID_STATUS_DEMANDA_PRINCIPAL" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ACCION" class="control-label"></label>
                <input asp-for="ACCION" class="form-control" />
                <span asp-validation-for="ACCION" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Registrar" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>
<br />
<div>
    <a asp-action="listar_demanda_principal" class="btn btn-toolbar">Cancelar</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
